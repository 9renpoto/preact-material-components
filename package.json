  "description": "preact wrapper for \"Material Components for the web\"",
  "module": "index.js",
  "main": "index.js",
  "scripts": {
    "build": "ts-node scripts/compile.ts",
    "watch": "ts-node scripts/compile.ts --watch",
    "lint": "tslint -p tsconfig.base.json",
    "lint:fix": "tslint -p tsconfig.base.json --fix",
    "prepublishOnly": "yarn build && yarn run test",
    "prettier:fix": "prettier {,packages/**/*.tsx} --write",
    "test": "yarn lint && yarn build",
    "prepare": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prateekbh/preact-material-components.git"
  },
