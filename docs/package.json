{
  "name": "preact-material-components-site",
  "version": "1.0.1",
  "description": "preact material component's site",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build",
    "build:prod": "preact build --template template.html && npm run copyAssets",
    "copyAssets": "cpx -v '{*.png,manifest.json,prerender-urls.json}' build && cpx -v 'images/**' build/images/",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "test": "yarn build:prod && yarn test:mocha",
    "test:mocha": "mocha $NODE_DEBUG_OPTION ./tests/visdiff.js --async-only --exit",
    "test:travis": "node ./scripts/test-travis.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prateekbh/preact-material-components.git"
  },
  "keywords": [
    "preact",
    "preact components",
    "material components",
    "material design"
  ],
  "author": {
    "name": "Prateek Bhatnagar",
    "email": "prateek89born@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prateekbh/preact-material-components/issues"
  },
  "homepage": "https://github.com/prateekbh/preact-material-components#readme",
  "devDependencies": {
    "archiver": "^2.1.1",
    "babel-eslint": "^7.2.3",
    "chalk": "^2.4.1",
    "classnames": "^2.2.5",
    "cpx": "^1.5.0",
    "create-firefox-profile": "^1.0.0",
    "eslint": "^4.6.1",
    "finalhandler": "^1.1.1",
    "foxr": "^0.7.0",
    "highlight.js": "^9.12.0",
    "http-server": "^0.11.0",
    "if-env": "^1.0.0",
    "lodash.compact": "^3.0.1",
    "mocha": "^5.0.1",
    "node-sass": "^4.9.3",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "serve": "^6.5.1",
    "serve-static": "^1.13.2",
    "shelljs": "^0.8.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "preact": "^8.2.9",
    "preact-cli": "^2.2.1",
    "preact-router": "^2.6.1"
  }
}
